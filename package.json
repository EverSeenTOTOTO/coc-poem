{
  "name": "@everseen/coc-poem",
  "version": "0.0.1-alpha.0",
  "license": "MIT",
  "engines": {
    "coc": "^0.0.80"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "coc.nvim",
    "poem",
    "vim",
    "neovim"
  ],
  "dependencies": {
    "puppeteer-core": "^13.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/is": "^0.0.21",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-jest": "^26.6.3",
    "coc.nvim": "^0.0.80",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "husky": ">=6",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "nodemon": "^2.0.7",
    "rollup": "^2.52.7",
    "rollup-plugin-terser": "^7.0.2",
    "tsc-alias": "^1.4.1",
    "typescript": "^4.1.2",
    "vscode-languageserver-protocol": "^3.16.0"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  },
  "scripts": {
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "activationEvents": [
    "onCommand:poem.showOutputChannel",
    "onCommand:poem.clearOutputChannel",
    "onCommand:poem.show",
    "onCommand:poem.boot",
    "onCommand:poem.fetch"
  ],
  "contributes": {
    "commands": [
      {
        "title": "Show logs",
        "command": "poem.showOutputChannel",
        "category": "poem"
      },
      {
        "title": "Clear logs",
        "command": "poem.clearOutputChannel",
        "category": "poem"
      },
      {
        "title": "Display poem screen",
        "command": "poem.show",
        "category": "poem"
      },
      {
        "title": "Display poem screen on vim startup",
        "command": "poem.show",
        "category": "poem"
      },
      {
        "title": "Immediately fetch poem provider data",
        "command": "poem.fetch",
        "category": "poem"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "poem",
      "properties": {
        "poem.updateTime": {
          "type": "number",
          "default": 60000,
          "description": "When to start fetch in background"
        },
        "poem.screenFPS": {
          "type": "number",
          "default": 2,
          "description": "How many times the screen will be updated per second"
        },
        "poem.providersDir": {
          "type": "string",
          "description": "The directory to store poem providers"
        },
        "poem.launchBrowser": {
          "type": "object",
          "description": "Puppeteer browser launch options"
        }
      }
    }
  }
}
